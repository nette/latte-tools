Test: macros allow trailing commas in their argument and parameter list

{import $_self}

{define test, $a, $b}
    {$a ?? a}<br />
    {$b ?? b}<br />
{/define}
{define test2, $a, $b}
    {$a ?? a}<br />
    {$b ?? b}<br />
{/define}

{include test, 1, 2}
{include test, 3, 4}
{include test2, 5, 6}
{include test2, 7, 8}
